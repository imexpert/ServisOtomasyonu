        $(document).ready(function () {
            var token = localStorage.getItem("token");
            if (token == null || token == undefined || token == "undefined") {
                window.location.href = "/Login/Index";
            }

            var parsedToken = parseJwt(token);

            $("#kullaniciAd").html(parsedToken.Ad);
            $("#kullaniciGorev").html(parsedToken.FirmaUnvan);
            $("#kullaniciAdFirstLetter").html(parsedToken.Ad[0]);
            $("#kullaniciAdSoyad").html(parsedToken.AdSoyad);
            $("#kullaniciGorevHtml").text(parsedToken.Gorev);
            $("#kullaniciEPosta").html(parsedToken.Email);

            // hataMesajiGoster();
        });

        function b64DecodeUnicode(str) {
            // Going backwards: from bytestream, to percent-encoding, to original string.
            return decodeURIComponent(atob(str).split('').map(function (c) {
                return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);
            }).join(''));
        }

        function parseJwt(token) {
            var base64Url = token.split('.')[1];
            if (base64Url != undefined) {
                var base64 = base64Url.replace(/-/g, '+').replace(/_/g, '/');
                var jsonPayload = decodeURIComponent(atob(base64).split('').map(function (c) {
                    return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);
                }).join(''));

                return JSON.parse(jsonPayload);
            }
            return null;
        };

        function cikisYap() {
            localStorage.clear();
            window.location.href = "/Login/Index";
        }
    </script>

